<view class="container"><view class="img"><block wx:if="{{show===true}}"><image src="{{'https://m.zjjiyu.cn/dongtou_park/'+usersImg}}"></image></block><block wx:else><image src="../../static/login/user.png"></image></block><text value="{{Musername}}" data-event-opts="{{[['input',[['__set_model',['','Musername','$event',[]]]]]]}}" bindinput="__e">{{Musername}}</text></view><view class="btn"><button value="{{show}}" data-event-opts="{{[['tap',[['getTrue',['$event']]]],['input',[['__set_model',['','show','$event',[]]]]]]}}" style="{{'background-color:'+(show=='1'?'#569aff':'')+';'}}" bindtap="__e" bindinput="__e">有效</button><button value="{{show}}" data-event-opts="{{[['tap',[['getFalse',['$event']]]],['input',[['__set_model',['','show','$event',[]]]]]]}}" style="{{'background-color:'+(show=='2'?'#569aff':'')+';'}}" bindtap="__e" bindinput="__e">失效</button></view><block wx:for="{{information}}" wx:for-item="list" wx:for-index="i" wx:key="id"><view class="main"><view class="header"><view class="top"><text>{{list.carnum}}</text></view><view class="font"><block wx:if="{{showDelete}}"><text data-event-opts="{{[['tap',[['deletelist',['$0','$1','$2','$3'],[[['information','id',list.id,'id']],[['information','id',list.id,'otherId']],[['information','id',list.id,'beginTime']],[['information','id',list.id,'endTime']]]]]]]}}" bindtap="__e">删除</text></block></view></view><view class="box"><view class="left _ul"><view class="_li"><text>车主姓名:</text><text>{{list.userName}}</text></view><view class="_li"><text space="nbsp">开始时间:</text><text sty="{{true}}">{{list.beginTime}}</text></view><view class="_li"><text space="nbsp">结束时间:</text><text>{{list.endTime}}</text></view></view></view></view></block><view class="add"><button plain="{{sw}}" data-event-opts="{{[['tap',[['goadd']]]]}}" bindtap="__e">+ 添加车辆</button></view><view class="txt"><text style="color:red;">{{"限制"+count+"个车辆"}}</text></view></view>