<view class="container"><view class="img"><image src="{{'https://m.zjjiyu.cn/dongtou_park/'+usersImg}}"></image><text value="{{Musername}}" data-event-opts="{{[['input',[['__set_model',['','Musername','$event',[]]]]]]}}" bindinput="__e">{{Musername}}</text></view><block wx:for="{{information}}" wx:for-item="list" wx:for-index="i" wx:key="id"><view class="main"><view class="header"><view class="top"><text>{{list.carnum}}</text></view><view class="font"><text data-event-opts="{{[['tap',[['deletelist',['$0','$1','$2','$3'],[[['information','id',list.id,'id']],[['information','id',list.id,'otherId']],[['information','id',list.id,'beginTime']],[['information','id',list.id,'endTime']]]]]]]}}" bindtap="__e">删除</text></view></view><view class="box"><view class="left _ul"><view class="_li"><text>车主姓名:</text><text>{{list.userName}}</text></view><view class="_li"><text space="nbsp">开始时间:</text><text sty="{{true}}">{{list.beginTime}}</text></view><view class="_li"><text space="nbsp">结束时间:</text><text>{{list.endTime}}</text></view></view></view></view></block><view class="add"><button plain="{{sw}}" data-event-opts="{{[['tap',[['goadd']]]]}}" bindtap="__e">+ 添加车辆</button></view><view class="txt"><text style="color:red;">{{"限制"+count+"个车辆"}}</text></view></view>